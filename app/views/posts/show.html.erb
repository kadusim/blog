<div class="container-fluid">
 
<h2><%= @post.titulo %></h2>
<p><%= @post.conteudo %></p>
<p><%= image_tag @post.foto.url(:medium) %></p>
<p><%= image_tag @post.foto.url(:thumb) %></p>
<p><%= @post.destaque %></p>
 
<%= link_to 'Editar Post', edit_post_path(@post) %> |
<%= link_to 'Voltar aos Posts', posts_path %>

</div>